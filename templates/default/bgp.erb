filter "<%= @name %>" { 
<% @net.each do |i| %>
  if ( net = <%= @i.network %>) then {
        print "<%= @i.condition %>: ", <%= @i.network %>;
        <%= @i.condition %>
  }
<% end %>  
  reject;
}

## BGP
protocol bgp {
        local as 65019;                      # Use a private AS number
        neighbor 10.101.3.13 as 65022;    # Our neighbor ...
        #multihop;                            # ... which is connected indirectly
        export abc;
        import all;
}